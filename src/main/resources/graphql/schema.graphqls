type schema {
  query: Query
}
type Query{
	allClaimsFromService:[ClaimDental]
	allMedicalClaimsFromService:[ClaimMedical]
	getThisPersonMedicalClaims(memberId:ID!):[ClaimMedical]
	getThisPersonDentalClaims(memberId:ID!):[ClaimDental]
}

type ClaimDental{
	dentalId:ID
	claimType : String
	pharmacyClaimStatus: String
	memberId : Long
	memberName : String
}


type ClaimMedical{
	medicalId:ID
	claimType : String
	pharmacyClaimStatus: String
	memberId : Long
	memberName : String
}
   
	    


